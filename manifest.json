{

  "manifest_version": 2,
  "name": "Onelook Search Plugin",
  "description": "Allows the user to search selected text on a webpage directly on Onelook dictionary",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  
  "icons": {
    "16": "icons/onelook-16.png",
    "96": "icons/onelook-96.png",
  }

}
